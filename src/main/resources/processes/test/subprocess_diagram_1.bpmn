    email: "c@example.com",
  },
];

users;</bpmn:script>
    </bpmn:scriptTask>
    <bpmn:scriptTask id="Activity_1amuiq3" name="Gán thông tin người dùng" scriptFormat="JavaScript" camunda:resultVariable="outputObj">
