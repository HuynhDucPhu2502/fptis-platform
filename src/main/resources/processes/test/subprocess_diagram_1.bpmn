      <bpmn:outgoing>Flow_0u8ygyf</bpmn:outgoing>
      <bpmn:script>var users = [
  {
    id: 1,
    name: "Nguyen Van A",
    email: "a@example.com",
  },
  {
    id: 2,
    name: "Tran Thi B",
    email: "b@example.com",
  },
