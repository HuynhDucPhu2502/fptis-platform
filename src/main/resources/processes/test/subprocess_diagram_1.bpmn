      <bpmn:outgoing>Flow_1t4v4u5</bpmn:outgoing>
      <bpmn:script>var i, foundUser = null;

for (i = 0; i &lt; users.length; i++) {
  if (users[i].id === inputObj.id) {
    foundUser = users[i];
    break;
  }
}

if (foundUser) {
  inputObj.name = foundUser.name;
  inputObj.email = foundUser.email;
}

inputObj;</bpmn:script>
